import wollok.game.*
import example.*

program pacman {
	
	game.width(14)
	game.height(8)
	
	game.boardGround("pacmanBackground.jpg")
	
	game.title("Pacman!")
	
	game.addVisualCharacter(pacman)
	game.addVisualCharacter(new Rival(numero = 1))
	game.addVisualCharacter(new Rival(numero = 2))
	
	game.start()
}